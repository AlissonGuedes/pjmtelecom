.dataTables_wrapper table.dataTable.display tbody tr.hover > .sorting_1, .dataTables_wrapper table.dataTable.display tbody tr:hover > .sorting_1, .dataTables_wrapper table.dataTable.order-column.hover tbody tr.hover > .sorting_1, .dataTables_wrapper table.dataTable.order-column.hover tbody tr:hover > .sorting_1 {
